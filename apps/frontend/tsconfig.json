{
    "extends": "@repo/tsconfig",
    "compilerOptions": {
        "lib": ["dom", "dom.iterable", "esnext"],
        "noEmit": true,
        "jsx": "react-jsx",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["src"]
}
